                                            <div class="col-lg-12 col-sm-12 col-12">
                                                <div class="d-flex gap-2 justify-content-end">
                                                    <button class="btn btn-sm btn-primary float-end" id="btnOrganizationsSubmit">
                                                        <i class="bi bi-save p-2"></i>SAVE
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- card-footer -->
                                    </div> <!-- card -->
                                </form>
                              </div>
                            </div>
                        </div>
                            
                          
                          
                          <div class="accordion-item">
                            <h2 class="accordion-header" id="headingTwo">
                              <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                Account Details
                              </button>
                            </h2>
                            <div id="collapseTwo" class="accordion-collapse collapse" aria-labelledby="headingTwo" data-bs-parent="#accordionExample">
                              <div class="accordion-body">
                                <form method="POST" action="/view/module/usermanagement/account?selectedUserId=#(selectedUserId)&tabid=details" class="">
                                    <div class="card border-0">
                                        <!--
                                        <div class="card-header">
                                            <div class="clearfix">
                                                <label class="form-label">User Organizations</label>
                                                <button class="btn btn-sm btn-primary float-end" id="btnOrganizationsSubmit">
                                                    <i class="bi bi-save p-2"></i>SAVE
                                                </button>
                                            </div>
                                        </div>
                                        -->
                                        <div class="card-body">
                                             <!--  -->
                                            <div class="row gx-3">
                                                <div class="col-lg-3 col-sm-4 col-12">
                                                    <div class="mb-3">
                                                        <label class="form-label">Name</label>
                                                        <input id="caption" name="caption" type="text" class="form-control" placeholder="Enter fullname" value="#(userAccountData.caption)" />
                                                    </div>
                                                </div>
                                                <div class="col-lg-3 col-sm-4 col-12">
                                                    <div class="mb-3">
                                                        <label class="form-label">Email</label>
                                                        <input id="email" name="email" type="email" class="form-control" placeholder="Enter email address" value="#(userAccountData.email)"/>
                                                    </div>
                                                </div>
                                                <div class="col-lg-3 col-sm-4 col-12">
                                                    <div class="mb-3">
                                                        <label class="form-label">Password</label>
                                                        <input id="password_hash" name="password_hash" type="password" class="form-control" placeholder="Password" disabled />
                                                    </div>
                                                </div>
                                            </div>
                                             <!--  -->
                                        </div> <!-- card-body -->
                                        <div class="card-footer">
                                            <div class="col-lg-12 col-sm-12 col-12">
                                                <div class="d-flex gap-2 justify-content-end">
                                                    <button class="btn btn-sm btn-primary float-end" id="btnOrganizationsSubmit">
                                                        <i class="bi bi-save p-2"></i>SAVE
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- card-footer -->
                                    </div> <!-- card -->
                                </form>
                              </div>
                            </div>
                          </div>
                          
                          
                          <div class="accordion-item">
                            <h2 class="accordion-header" id="headingThree">
                              <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                Permissions
                              </button>
                            </h2>
                            <div id="collapseThree" class="accordion-collapse collapse" aria-labelledby="headingThree" data-bs-parent="#accordionExample">
                              <div class="accordion-body">
                                
                                <!-- -->
                                <div class="row gx-3">
                                    <!-- Col 1 -->
                                    <div class="col-sm-6 col-12">
                                        <div class="card">
                                            <div class="card-body">
                                                <ul class="list-group">
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        Administrator
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleAdministrator"
                                                                    #if(!selectedUserPermissions.isSuperUser):disabled#endif
                                                                    value="#(selectedUserPermissions.IsAdminUser)"
                                                                    #if(selectedUserPermissions.IsAdminUser):checked#endif
                                                                    onchange="handlePermissionsChange('isAdminUser', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        User
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleUser"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isUser)"
                                                                    #if(selectedUserPermissions.isUser):checked#endif
                                                                    onchange="handlePermissionsChange('isUser', event)" />
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        <i>Super User</i>
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleSuperUser"
                                                                    disabled
                                                                    #if(selectedUserPermissions.isSuperUser):checked#endif
                                                                    onchange="handlePermissionsChange('isSuperUser', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        System User
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleSystemUser"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isSystemUser)"
                                                                    #if(selectedUserPermissions.isSystemUser):checked#endif
                                                                    onchange="handlePermissionsChange('isSystemUser', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        API User
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleApiUser"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isApiUser)"
                                                                    #if(selectedUserPermissions.isApiUser):checked#endif
                                                                    onchange="handlePermissionsChange('isApiUser', event)"/>
                                                        </div>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                
                                    <!-- Col 2 -->
                                    <div class="col-sm-6 col-12">
                                        <div class="card">
                                            <div class="card-body">

                                                <ul class="list-group">
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        User Management
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleUserManagement"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isAllowedToUseUserManagementModule)"
                                                                    #if(selectedUserPermissions.isAllowedToUseUserManagementModule):checked#endif
                                                                    onchange="handlePermissionsChange('isAllowedToUseUserManagementModule', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        Time Management
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleTimeManagement"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isAllowedToUseTimeManagementModule)"
                                                                    #if(selectedUserPermissions.isAllowedToUseTimeManagementModule):checked#endif
                                                                    onchange="handlePermissionsChange('isAllowedToUseTimeManagementModule', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        Event Management
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleEventManagement"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isAllowedToUseEventManagementModule)"
                                                                    #if(selectedUserPermissions.isAllowedToUseEventManagementModule):checked#endif
                                                                    onchange="handlePermissionsChange('isAllowedToUseEventManagementModule', event)"/>
                                                        </div>
                                                    </li>
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        Task Management
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch" id="switchRoleTaskManagement"
                                                                    #if(!selectedUserPermissions.IsAdminUser):disabled#endif
                                                                    value="#(selectedUserPermissions.isAllowedToUseTaskManagementModule)"
                                                                    #if(selectedUserPermissions.isAllowedToUseTaskManagementModule):checked#endif
                                                                    onchange="handlePermissionsChange('isAllowedToUseTaskManagementModule', event)"/>
                                                        </div>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- -->
                                
                              </div>
                            </div>
                          </div>
                          
                          
                        <div class="accordion-item">
                            <h2 class="accordion-header" id="headingFour">
                              <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
                                Settings
                              </button>
                            </h2>
                            <div id="collapseFour" class="accordion-collapse collapse" aria-labelledby="headingFour" data-bs-parent="#accordionExample">
                              <div class="accordion-body">
                                 <!-- -->
                                 <div class="row gx-3">
                                    <!-- Col 1 -->
                                    <div class="col-sm-6 col-12">
                                        <div class="card">
                                            <div class="card-body">
                                                <ul class="list-group">
                                                    <li class="list-group-item d-flex justify-content-between align-items-center">
                                                        Show Messages
                                                        <div class="form-check form-switch m-0">
                                                            <input class="form-check-input" type="checkbox" role="switch"
