        function handlePermissionsChange(name, e) {
            handleChange(name, "/view/module/usermanagement/permissions", e)
        }
        
        function handleSettingsChange(name, e) {
            handleChange(name, "/view/module/usermanagement/settings", e)
        }
        
        function handleChange(name, uri, e) {
            const {checked} = e.target;

            $.post( uri,
                {
                    key: name,
                    value: checked,
                    userId: "#(settings.userId)",
                    selectedUserId: "#(selectedUserId)"
                })
                .done(function( data ) {
                    Toastify({
                        text: data,
                        duration: 3000,
                        close: true,
                        avatar: '/assets/favicon_io/apple-touch-icon.png',
                        className: "bg-success",
                        offset: {
                            x: 9,
                            y: 3,
                        },
                        }).showToast();
                })
                .fail(function(jqXHR, textStatus, errorThrown) {
                    Toastify({
                        text: textStatus,
                        duration: 3000,
                        close: true,
                        avatar: '/assets/favicon_io/apple-touch-icon.png',
                        className: "bg-danger",
                        offset: {
                            x: 9,
                            y: 3,
                        },
                    }).showToast();
                })
                .always(function() {
                    //alert( "finished" );
                });
        }
        
        
