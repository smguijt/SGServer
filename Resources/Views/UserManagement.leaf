                                        <div class="card-header">
                                            <div class="clearfix">
                                                <label class="form-label">User Organizations</label>
                                                <button class="btn btn-sm btn-primary float-end" id="btnOrganizationsSubmit">
                                                    <i class="bi bi-save p-2"></i>SAVE
                                                </button>
                                            </div>
                                        </div>
                                        -->
                                        <div class="card-body">
                                            <div class="row gx-3">
                                                <label class="form-label">User Organizations</label>
                                                <div class="col-lg-6 col-sm-6 col-12 p3- mt-2 mb-3">
                                                    <select id="organizations"
                                                            name="organizations"
                                                            multiple="multiple">
                                                        #for(userOrganization in userOrganizations):
                                                            <option value="#(userOrganization.code)"
